{
  "name": "hnp-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev-https": "local-ssl-proxy --key ./.keys/local.key --cert ./.keys/local.crt --source 3001 --target 3000",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "build:development": "BUILD_ENV=development next build",
    "build:production": "BUILD_ENV=production next build",
    "start": "next start -p $PORT",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "cert": "mkcert -cert-file ./.keys/local.crt -key-file ./.keys/local.key my-ticket.local '*.my-ticket.local' localhost 127.0.0.1",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/css": "^11.7.1",
    "@emotion/react": "^11.5.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@hookform/resolvers": "2.8.1",
    "@mui/icons-material": "^5.0.3",
    "@mui/lab": "^5.0.0-alpha.50",
    "@mui/material": "^5.0.4",
    "@mui/styles": "^5.0.1",
    "@stripe/react-stripe-js": "^1.6.0",
    "@stripe/stripe-js": "^1.19.1",
    "autosuggest-highlight": "^3.1.1",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "google-map-react": "^2.1.10",
    "http-proxy-middleware": "^2.0.1",
    "i18n-iso-countries": "^7.2.0",
    "i18next": "^21.2.4",
    "i18next-http-backend": "^1.3.1",
    "jspdf": "^2.4.0",
    "konva": "^8.2.1",
    "luxon": "^2.0.1",
    "next": "12.1.6",
    "next-i18next": "^8.7.0",
    "next-iron-session": "^4.2.0",
    "query-string": "^7.0.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-countdown": "^2.3.2",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-hook-form": "7.15.4",
    "react-i18next": "^11.12.0",
    "react-icons": "^4.3.1",
    "react-id-swiper": "^4.0.0",
    "react-konva": "^17.0.2-5",
    "react-lazy-load-image-component": "^1.5.1",
    "simplebar": "^5.3.6",
    "simplebar-react": "^2.3.6",
    "slick-carousel": "^1.8.1",
    "swiper": "^7.3.1",
    "swr": "^1.0.0",
    "use-image": "^1.0.8",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.7.2",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@types/autosuggest-highlight": "^3.1.1",
    "@types/cors": "^2.8.10",
    "@types/luxon": "^2.0.4",
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "@types/react-rte": "^0.16.3",
    "@types/react-slick": "^0.23.8",
    "@types/swiper": "^5.4.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-twin": "^1.0.2",
    "eslint": "7.32.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.2",
    "jira-prepare-commit-msg": "^1.6.0",
    "lint-staged": "^11.2.0",
    "local-ssl-proxy": "^1.3.0",
    "nodemon": "^2.0.13",
    "prettier": "^2.3.1",
    "react-is": "^17.0.2",
    "tailwindcss": "^2.2.16",
    "twin.macro": "^2.8.2",
    "typescript": "4.6.4"
  },
  "jira-prepare-commit-msg": {
    "messagePattern": "$J: $M",
    "isConventionalCommit": true
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  }
}
